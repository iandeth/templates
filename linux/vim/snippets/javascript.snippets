snippet var
	var ${1} = ${2};
snippet func
	function ${1:name} (${2}){
		${3}
	}
snippet af
	function (${1}){
		${2}
	}
snippet vf
	var ${1} = function (${2}){
		${3}
	};
snippet log
	console.log(${1});
snippet err
	console.err(${1});
snippet $
	$('${1}')${2}
snippet ready
	$(function(){
		${2}
	});
